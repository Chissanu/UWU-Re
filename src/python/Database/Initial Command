# Log in as admin to UWURe DB
psql -U admin -d uwure

# Admin login password
uwure

# To check table lists
\dt

# To drop tables
drop table <tableName>;

# To drop database
drop database <databaseName>;

# To create drink_tables table in Postgres
# drinkName | drinkID(Auto generated) | ingredientList | drinkList
create table drink_tables(drinkName VARCHAR(255),drinkID BIGSERIAL PRIMARY KEY,ingredientList VARCHAR[6],drinkList INT[6]);
